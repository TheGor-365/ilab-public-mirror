<% @generation = Generation.find(params[:id]) if @generation.present? %>

<%= form_with(model: @generation, html: { multipart: true }) do |form| %>

  <div class="row p-5 shadow bg-white mb-5">

    <%= form.label :title, class: 'display-6 mb-5 text-decoration-none text-muted' %>
    <%= form.text_field :title, class: 'form-control' %>
  </div>
<% end %>
