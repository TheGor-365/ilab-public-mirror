<% generation = Generation.new %>

<%= form_with(model: generation, html: { multipart: true }) do |form| %>

  <div class="row p-5 shadow bg-white mb-5">

    <%= form.label :production_period, class: 'anchorjs-link display-6 mb-5', id: 'generation-production-period' %>
    <%= form.text_field :production_period, class: 'form-control' %>
  </div>
<% end %>
